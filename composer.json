{
    "name": "jworman/zf1",
    "description": "Monorepo for zf1s (Zend Framework 1) packages",
    "license": "BSD-3-Clause",
    "require": {
        "php": "~8.3",
        "ext-ctype": "*",
        "ext-dom": "*",
        "ext-gd": "*",
        "ext-iconv": "*",
        "ext-ldap": "*",
        "ext-mbstring": "*",
        "ext-reflection": "*",
        "ext-session": "*",
        "ext-simplexml": "*",
        "ext-spl": "*",
        "ext-xml": "*",
        "ext-zlib": "*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.53.0",
        "jworman/dbunit": "5.0.0",
        "jworman/phpunit": "1.0.0",
        "php-parallel-lint/php-parallel-lint": "1.3.0",
        "staabm/annotate-pull-request-from-checkstyle": "1.5.0"
    },
    "autoload": {
        "psr-0": {
            "Zend_Acl": "packages/zend-acl/library/",
            "Zend_Cache": "packages/zend-cache/library/",
            "Zend_Config": "packages/zend-config/library/",
            "Zend_Controller": "packages/zend-controller/library/",
            "Zend_Date": "packages/zend-date/library/",
            "Zend_Db": "packages/zend-db/library/",
            "Zend_Dom": "packages/zend-dom/library/",
            "Zend_Exception": "packages/zend-exception/library/",
            "Zend_File": "packages/zend-file/library/",
            "Zend_File_Transfer": "packages/zend-file-transfer/library/",
            "Zend_Filter": "packages/zend-filter/library/",
            "Zend_Form": "packages/zend-form/library/",
            "Zend_Http": "packages/zend-http/library/",
            "Zend_Json": "packages/zend-json/library/",
            "Zend_Layout": "packages/zend-layout/library/",
            "Zend_Loader": "packages/zend-loader/library/",
            "Zend_Locale": "packages/zend-locale/library/",
            "Zend_Log": "packages/zend-log/library/",
            "Zend_Mail": "packages/zend-mail/library/",
            "Zend_Measure": "packages/zend-measure/library/",
            "Zend_Mime": "packages/zend-mime/library/",
            "Zend_Mobile": "packages/zend-mobile/library/",
            "Zend_Navigation": "packages/zend-navigation/library/",
            "Zend_Paginator": "packages/zend-paginator/library/",
            "Zend_Queue": "packages/zend-queue/library/",
            "Zend_Registry": "packages/zend-registry/library/",
            "Zend_Server": "packages/zend-server/library/",
            "Zend_Session": "packages/zend-session/library/",
            "Zend_Tag": "packages/zend-tag/library/",
            "Zend_Test": "packages/zend-test/library/",
            "Zend_Text": "packages/zend-text/library/",
            "Zend_TimeSync": "packages/zend-timesync/library/",
            "Zend_Translate": "packages/zend-translate/library/",
            "Zend_Uri": "packages/zend-uri/library/",
            "Zend_Validate": "packages/zend-validate/library/",
            "Zend_Version": "packages/zend-version/library/",
            "Zend_View": "packages/zend-view/library/",
            "Zend_Xml": "packages/zend-xml/library/"
        }
    },
    "scripts": {
        "lint": "parallel-lint --exclude vendor --exclude tests/Zend/Loader/_files/ParseError.php .",
        "lint.checkstyle": "@lint --checkstyle",
        "test": "phpunit",
        "php-cs-fixer": "vendor/bin/php-cs-fixer fix"
    },
    "config": {
        "sort-packages": true
    }
}
